{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "require": {
        "monolog/monolog": "2.0.*",
        "drupal/core-recommended": "9.4.8",
        "drupal/core-composer-scaffold": "9.4.8",
        "drupal/core-project-message": "9.4.8",
        "drupal/lazy": "^3.0",
        "bower-asset/lazysizes": "^5.1",
        "oomphinc/composer-installers-extender": "^2.0",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/adminimal_admin_toolbar": "^1.10",
        "drupal/adminimal_theme": "^1.5",
        "drupal/advagg": "^4.1",
        "drupal/block_class": "^1.3",
        "drupal/config_override_warn": "^1.2",
        "drupal/config_split": "^1.4",
        "drupal/devel": "^4.1",
        "drupal/diff": "^1.0",
        "drupal/environment_indicator": "^3.7",
        "drupal/field_group": "^3.0",
        "drupal/focal_point": "^1.5",
        "drupal/gliderjs": "^1.0@RC",
        "drupal/google_tag": "^1.4",
        "drupal/honeypot": "^2.0",
        "drupal/login_security": "^2.0",
        "drupal/metatag": "^1.15",
        "drupal/minifyhtml": "^1.9",
        "drupal/module_filter": "^3.1",
        "drupal/password_policy": "^3.0@beta",
        "drupal/pathauto": "^1.6",
        "drupal/redirect": "^1.5",
        "drupal/redis": "^1.4",
        "drush/drush": "^10.2",
        "erusev/parsedown": "^1.7",
        "kint-php/kint": "^3.3",
        "predis/predis": "^1.1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
 
    "require-dev": {
        "drupal/coder": "^8.3",
        "friendsoftwig/twigcs": "^5.0",
        "squizlabs/php_codesniffer": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
              "[web-root]/sites/development.services.yml": false,
              "[web-root]/.eslintrc.json": false
          }
        },
        "patches": {
            "drupal/adminimal_theme": {
                "3127508 - Add same \"Add to Shortcuts\" star button shortcut as in Seven theme.": "https://www.drupal.org/files/issues/2020-05-05/adminimal_theme-add_to_shortcuts-3127508-5.patch"
            },
            "drupal/password_policy": {
                "2983448 - Refactor hook_install logic to expire passwords for existing users": "https://www.drupal.org/files/issues/2020-03-23/Do_not_set_user_fields_on_install-2983448-17.patch"
            },
            "drupal/pathauto": {
                "3186627 - Type of Drupal\\pathauto\\Form\\PatternEditForm::$entityTypeManager must be Drupal\\Core\\Entity\\EntityTypeManagerInterface": "https://www.drupal.org/files/issues/2020-12-07/pathauto-3186627.patch"
            },
            "drupal/redis": {
                "2900947 - Implement initial RedisCluster client integration": "https://www.drupal.org/files/issues/2021-01-14/Implement_initial_RedisCluster_client_integration-2900947-35.patch"
            },
            "drupal/core": {
                "3192650 - Media name not visible on grid": "https://www.drupal.org/files/issues/2021-01-14/drupal-media_name_not_visible_grid-3192650-3-D8.9.patch"
            },
            "drupal/devel": {
                "Brings back the Available Methods & Properties tabs for Kint dumps — Issue #221": "https://raw.githubusercontent.com/politsin/snipets/master/patch/kint.patch"
            }
        },
        "installer-types": [
          "npm-asset"
        ],
        "installer-paths": {
          "web/core": [
            "type:drupal-core"
          ],
          "web/modules/contrib/{$name}": [
              "type:drupal-module"
          ],
          "web/profiles/contrib/{$name}": [
              "type:drupal-profile"
          ],
          "web/themes/contrib/{$name}": [
              "type:drupal-theme"
          ],
          "drush/Commands/contrib/{$name}": [
              "type:drupal-drush"
          ],
          "web/modules/custom/{$name}": [
              "type:drupal-custom-module"
          ],
          "web/themes/custom/{$name}": [
              "type:drupal-custom-theme"
          ],
          "web/libraries/{$name}": [
              "type:drupal-library",
              "type:npm-asset"
          ]
        },
        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, you’ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
